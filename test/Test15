/*Test polymorphism*/
class Animal {
    constructor Animal(){

    }

    void printName(Animal self){
        
    }
}

class Cat extends Animal{
    constructor Cat(){

    }

    void printName(Cat self){
        print("Cat");
    }
}

class Bird extends Animal{
    constructor Bird(){

    }

    void printName(Bird self){
        print("Bird");
    }
}

class Dog extends Animal{
    constructor Dog(){

    }

    void printName(Dog self){
        print("Dog");
    }
}



int main() {
    Animal c : = new Cat();
    Animal b : = new Bird();
    Animal d : = new Dog();

    c.printName();
    b.printName();
    d.printName();
}