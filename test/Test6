int main(){
    int i;
    while(i < 10){
        if(i == 6)
        {
            i = i + 2;
            continue;
        }
        print(i);
        i = i + 2;
    }


    i = 0;
    while(i < 10){
        if(i == 6)
        {
            break;
        }
        print(i);
        i = i + 2;
    }
}